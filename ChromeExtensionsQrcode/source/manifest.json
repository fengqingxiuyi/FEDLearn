{
    "name": "简易二维码",
    "version": "1.1.4",
    "manifest_version": 2,
    "description": "支持通过'回车'、'点击生成二维码按钮'、'右击'方式生成二维码；支持自定义开关上一次二维码的显示",
    "author": "青峰 qingfeng@showjoy.com",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": "images/icon19.png",
        "default_popup": "popup.html"
    },
    "permissions": [
      "contextMenus", "tabs"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    }
}
